orneklem_ort <- function(vektor, m){
n_ortalamalar <- choose(length(vektor),m)
ortalamalar <- rep(NA, n_ortalamalar)
for(i in 1:n_ortalamalar){
ortalamalar[i] <- mean(combn(vektor,m)[,i])
}
return(ortalamalar)
}
